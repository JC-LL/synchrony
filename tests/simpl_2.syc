circuit simpl_2
  input  a,b,c,d,e1,e2,e3
  output f : uint10
  output f1,f2,f3,f4,f5,f6,f7
  output g1,g2,g3,g4,g5,g6,g7

  f1 = a and b and c
  f2 = d and (e1 or e2)
  f3 = d and (e2 and e3)

  f4=!!!a
  f5= !1
  f6=not 1
  f7=not(1)

  g1 = /0
  g2 = a and b and 0
  g3 = 1 and 1
  g4 = a or b or (1 and 1)
end
